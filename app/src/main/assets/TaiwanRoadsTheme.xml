                    <lineSymbol src="assets:shields/trunk/84.svg" rotate="false" align-center="true" repeat="true" />
                </rule>
                <rule e="way" k="ref" v="86">
                    <lineSymbol src="assets:shields/trunk/86.svg" rotate="false" align-center="true" repeat="true" />
