                <rule e="way" k="ref" v="5">
                    <lineSymbol src="assets:shields/motorway/5.svg"
                        symbol-percent="85" rotate="false" align-center="true"
                        repeat="true" repeat-start="5" repeat-gap="250" />
                </rule>
                <rule e="way" k="ref" v="6">
                    <lineSymbol src="assets:shields/motorway/6.svg"
                        symbol-percent="85" rotate="false" align-center="true"
