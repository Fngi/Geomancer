                </rule>
                <rule e="way" k="*" v="*" zoom-min="16" zoom-max="18">
                    <line stroke="#707070" stroke-width="1.5" stroke-dasharray="1,3" />
                    <line stroke="#a0c0a0" stroke-width="1.3" stroke-dasharray="1,3" />
