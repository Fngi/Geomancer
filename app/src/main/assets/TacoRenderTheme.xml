<?xml version="1.0" encoding="UTF-8"?>
<rendertheme xmlns="http://mapsforge.org/renderTheme"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://mapsforge.org/renderTheme https://raw.githubusercontent.com/unknown"
    version="1">

    <rule e="way" k="natural" v="issea|sea">
        <area fill="#90b0c0" />
    </rule>

    <rule e="way" k="natural" v="nosea">
        <area fill="#f8f8f8" stroke="#f8f8f8" stroke-width="1.0" />
    </rule>

    <!-- 行政區 -->
    <rule e="way" k="boundary" v="administrative">
        <!-- 中華民國 -->
        <rule e="way" k="admin_level" v="2" zoom-min="7" zoom-max="7">
            <line stroke="#606060" stroke-width="0.4" stroke-dasharray="5,5" />
        </rule>
        <!-- 省、直轄市 | 縣、省轄市-->
        <rule e="way" k="admin_level" v="4|6" zoom-min="7" zoom-max="14">
            <line stroke="#606060" stroke-width="0.4" stroke-dasharray="5,1" />
        </rule>
        <!-- 直轄市所屬區 | 省轄市所屬區 | 鄉、鎮 -->
        <rule e="way" k="admin_level" v="5|7|8" zoom-min="11" zoom-max="14">
            <line stroke="#909090" stroke-width="0.4" stroke-dasharray="1,3" />
        </rule>
    </rule>

    <!-- ways -->
    <rule e="way" k="highway" v="*">
        <rule e="way" k="highway" v="motorway|motorway_link">

            <rule e="way" k="tunnel" v="~|no|false">
                <rule e="way" k="*" v="*" zoom-min="8" zoom-max="9">
                    <line stroke="#50a050" stroke-width="1.0" />
                </rule>
                <rule e="way" k="*" v="*" zoom-min="10" zoom-max="12">
                    <line stroke="#50a050" stroke-width="1.0" />
                </rule>
                <rule e="way" k="*" v="*" zoom-min="13" zoom-max="15">
                    <line stroke="#50a050" stroke-width="1.5" />
                </rule>
                <rule e="way" k="*" v="*" zoom-min="16" zoom-max="18">
                    <line stroke="#50a050" stroke-width="2.0" />
                </rule>
            </rule>

            <rule e="way" k="tunnel" v="true|yes">
                <rule e="way" k="*" v="*" zoom-min="8" zoom-max="9">
                    <line stroke="#50a050" stroke-width="1.0" stroke-dasharray="1,3" />
                </rule>
                <rule e="way" k="*" v="*" zoom-min="10" zoom-max="12">
                    <line stroke="#50a050" stroke-width="1.0" stroke-dasharray="1,3" />
                </rule>
                <rule e="way" k="*" v="*" zoom-min="13" zoom-max="15">
                    <line stroke="#50a050" stroke-width="1.5" stroke-dasharray="1,3" />
                </rule>
                <rule e="way" k="*" v="*" zoom-min="16" zoom-max="18">
                    <line stroke="#50a050" stroke-width="2.0" stroke-dasharray="1,3" />
                </rule>
            </rule>

            <rule e="way" k="*" v="*" zoom-min="8" zoom-max="18">
                <pathText k="ref" font-size="10" stroke="#303030" />
            </rule>

            <rule e="way" k="*" v="*" zoom-min="8" zoom-max="18">
                <rule e="way" k="ref" v="1">
                    <lineSymbol src="assets:shields/motorway/1.svg" align-center="true" repeat="true" />
                </rule>
                <rule e="way" k="ref" v="2">
                    <lineSymbol src="assets:shields/motorway/2.svg" align-center="true" repeat="true" />
